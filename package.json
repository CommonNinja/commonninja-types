  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CommonNinja/commonninja-node-utils/issues"
  },
  "homepage": "https://github.com/CommonNinja/commonninja-node-utils#readme",
  "dependencies": {
    "aws-sdk": "^2.620.0",
    "bunyan": "^1.8.12",
    "bunyan-debug-stream": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "elastic-apm-node": "^3.5.0",
    "express": "^4.16.4",
    "geoip-lite": "^1.3.6",
    "ioredis": "^4.6.2",
    "is-uuid": "^1.0.2",
