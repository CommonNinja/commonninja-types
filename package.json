{
  "name": "@commonninja/commonninja-node-utils",
  "version": "1.22.76",
  "description": "A utils lib for Common Ninja NodeJS services",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist",
    "scripts"
  ],
  "scripts": {
    "test": "jest",
    "lint": "tslint --config tslint.json './{lib}/**/*.ts'",
    "build": "rm -rf dist/* && npm run build:server:compile",
    "build:server:compile": "tsc",
    "prepare": "npm run lint && npm run build",
    "prepublish": "npm run test && npm run build",
    "prebuild": "node ./scripts/pre-build.js",
    "preinstall": "node ./scripts/pre-build.js",
    "start": "npm run test && npm run build && node dist/lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CommonNinja/commonninja-node-utils.git"
  },
  "keywords": [
